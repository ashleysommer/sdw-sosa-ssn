sosa-c: 
  a owl:Ontology ;
  dcterms:title "Sensor, Observation, Sample, and Actuator (SOSA) Ontology - collections"@en ;
  dcterms:description "This ontology is based on part of the SSN-ext Ontology by the W3C/OGC Spatial Data on the Web Working Group."@en ;
  dcterms:creator [ a foaf:Agent ; foaf:name "W3C/OGC Spatial Data on the Web Working Group"@en ] ;
  dcterms:rights "Copyright 2024 W3C/OGC." ;
  dcterms:license <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document> ;
  dcterms:license <http://www.opengeospatial.org/ogc/Software> ;
  dcterms:created "2023-10-24"^^xsd:date ;
  dcterms:source <https://www.w3.org/TR/vocab-ssn-ext/> ;
  skos:scopeNote "This ontology describes homogeneous collections, whose members all share at least one common property. Summarizing collections (see OMS) are not considered. " ;
  owl:imports sosa: .


sosa:hasMember 
  a owl:ObjectProperty ;
  rdfs:label "has member of collection" ;
  skos:definition "Link to a member within a collection of (samples or observations) or (collection of samples or collection of observations) that share the same value for one or more of the characteristic properties" ;
  rdfs:subPropertyOf rdfs:member ;
  skos:editorialNote "Note that there is a single `member` property. OWL or SHACL constraints may limit the range depending on the context of different collection types" ;
  schema:domainIncludes sosa:SampleCollection ;
  schema:domainIncludes sosa:ObservationCollection ;
  schema:domainIncludes sosa:HomogeneousSampleCollection ;
  schema:domainIncludes sosa:HomogeneousObservationCollection ;
  schema:rangeIncludes sosa:Sample ;
  schema:rangeIncludes sosa:SampleCollection ;
  schema:rangeIncludes sosa:HomogeneousSampleCollection ;
  schema:rangeIncludes sosa:Observation ;
  schema:rangeIncludes sosa:ObservationCollection ;
  schema:rangeIncludes sosa:HomogeneousObservationCollection ;
  rdfs:isDefinedBy sosa-c: .


## General Sample Collection 

sosa:SampleCollection 
  a owl:Class ;
  rdfs:label "General Sample Collection"@en ;
  skos:definition """Collection of one or more samples, which may be nested in further collections. 
  The values of the collection properties summarize the values of the properties of the member samples."""@en ;
  skos:note """The members of a general (summarizing) collection do not necessarily share a common value for any property. 
""" ;
  rdfs:isDefinedBy sosa-c: .

sosa:hasOriginalSample
  schema:domainIncludes sosa:SampleCollection .

sosa:hasSampledFeature
  schema:domainIncludes sosa:SampleCollection .

sosa:isSampleOf
  schema:domainIncludes sosa:SampleCollection .

sosa:isResultOf
  schema:domainIncludes sosa:SampleCollection .

## General Sample Collection axiomatization 

sosa:SampleCollection
  rdfs:subClassOf rdf:Bag ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasMember ; 
    owl:allValuesFrom [ 
      owl:unionOf ( 
        sosa:Sample 
        sosa:SampleCollection 
        sosa:HomogeneousSampleCollection 
      ) ; 
    ] 
  ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sosa:hasMember ; 
    owl:minCardinality "1"^^xsd:nonNegativeInteger 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasOriginalSample ; 
    owl:allValuesFrom sosa:Sample 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasSampledFeature ; 
    owl:allValuesFrom sosa:FeatureOfInterest 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:isSampleOf ; 
    owl:allValuesFrom sosa:FeatureOfInterest 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:isResultOf ; 
    owl:allValuesFrom sosa:Sampling 
  ] ;
  rdfs:isDefinedBy sosa-c: .


## Homogeneous Sample Collection

sosa:HomogeneousSampleCollection 
  a owl:Class ;
  rdfs:label "Homogeneous Sample Collection"@en ;
  skos:definition """Collection of one or more samples, which may be nested in further collections. 
  The members of a homogeneous collection share a common value for one or more property"""@en ;
  rdfs:isDefinedBy sosa-c: .

sosa:hasOriginalSample
  schema:domainIncludes sosa:HomogeneousSampleCollection .

sosa:hasSampledFeature
  schema:domainIncludes sosa:HomogeneousSampleCollection .

sosa:isSampleOf
  schema:domainIncludes sosa:HomogeneousSampleCollection .

sosa:isResultOf
  schema:domainIncludes sosa:HomogeneousSampleCollection .

## Homogeneous Sample Collection axiomatization 

sosa:HomogeneousSampleCollection
  rdfs:subClassOf sosa:SampleCollection ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasMember ; 
    owl:allValuesFrom [ 
      owl:unionOf ( 
        sosa:Sample 
        sosa:HomogeneousSampleCollection 
      ) ; 
    ] 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasOriginalSample ; 
    owl:maxCardinality "1"^^xsd:nonNegativeInteger 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasSampledFeature ; 
    owl:maxCardinality "1"^^xsd:nonNegativeInteger 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:isSampleOf ; 
    owl:maxCardinality "1"^^xsd:nonNegativeInteger 
  ] ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:isResultOf ; 
    owl:maxCardinality "1"^^xsd:nonNegativeInteger 
  ] ;
  rdfs:isDefinedBy sosa-c: .


