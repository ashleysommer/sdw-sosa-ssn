sosa:SampleCollection 
  rdfs:subClassOf rdf:Bag ;
  rdfs:subClassOf [ 
    a owl:Restriction ; 
    owl:onProperty sosa:hasMember ; 
    owl:allValuesFrom [ 
      owl:unionOf ( 
        sosa:Sample 
        sosa:SampleCollection 
      ) ; 
    ] 
  ] ;
  rdfs:isDefinedBy sosa-sam: .


## End of sampling schema
